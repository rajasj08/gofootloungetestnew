/* styles for product page email */

.product-block .pav-colorbox1.btn.btn-theme-default
{
	display:none;
}

.product-block .pav-colorbox1.btn.btn-theme-default:hover {
    color: #F58634;
    background-color: #fff;
    }
 .product-block .pav-colorbox1 {
    color: #4B4B4D;
    margin: auto;
    overflow: visible;
    z-index: 9999;
    font-size: 10px;
    text-transform: uppercase;
    padding-left: 12px;
    padding-right: 12px; 
   
    width: 100px;
   
    -webkit-transition: all .4s ease 0s;
    transition: all .4s ease 0s;
    display: block;
    }   

 




